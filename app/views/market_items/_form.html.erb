<%= form_for [@market_list, @market_item] do |f| %>
  <div class="field">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>

  <div class="field">
    <%= f.label :date %>
    <%= f.date_field :date %>
  </div>

  <div class="actions">
    <%= f.submit 'Criar Item de Mercado' %>
  </div>
<% end %>
  
  